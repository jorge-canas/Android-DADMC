<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dadmc.practica35crash" >
    <permission
        android:name="dadmc.practica35crash.CRASH_PERM"
        android:description="@string/crash_perm_desc"
        android:label="@string/crash_perm_label"
        android:protectionLevel="dangerous"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Crash"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:permission="dadmc.practica35crash.CRASH_PERM">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="dadmc.practica35crash.Crash.ACTION_CRASH" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
