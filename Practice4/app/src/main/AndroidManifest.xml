<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dadmc.pract4" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SeeFormActivity"
            android:label="@string/title_activity_see_form"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".ColorSelector"
            android:label="@string/title_activity_color_selector"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Form"
            android:label="@string/title_activity_form"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Game"
            android:label="@string/btnSudoku"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
